{
  "name": "",
  "version": "0.0.4",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./build/server.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "migrate": "npx dotenv -e .env -- npx prisma migrate dev",
    "migrate:prod": "npx dotenv -e .env.prod -- npx prisma migrate deploy"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.18",
    "@isaacs/express-prometheus-middleware": "^1.2.1",
    "@prisma/client": "^4.14.0",
    "@remix-run/css-bundle": "^1.16.0",
    "@remix-run/dev": "^1.16.1",
    "@remix-run/express": "^1.16.0",
    "@remix-run/node": "^1.16.0",
    "@remix-run/react": "^1.16.0",
    "@remix-run/server-runtime": "^1.16.0",
    "@remix-run/v1-meta": "^0.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "argon2": "^0.30.3",
    "aws-sdk": "^2.1377.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "canvas-confetti": "^1.6.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.8",
    "dayjs-plugin-utc": "^0.1.2",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "mock-aws-s3": "^4.0.2",
    "morgan": "^1.10.0",
    "nock": "^13.3.1",
    "node-ip": "^0.1.2",
    "prom-client": "^14.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-input-mask": "^2.0.4",
    "react-text-mask": "^5.5.0",
    "react-toastify": "^9.1.3",
    "recharts": "^2.7.2",
    "redis": "^4.6.7",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.6.2",
    "tailwindcss-animatecss": "^1.0.7",
    "text-mask-addons": "^3.8.0",
    "tiny-invariant": "^1.3.1",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@remix-run/eslint-config": "^1.16.0",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/canvas-confetti": "^1.6.0",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.37.0",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-input-mask": "^3.0.2",
    "@types/react-text-mask": "^5.4.11",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "autoprefixer": "^10.4.14",
    "c8": "^7.13.0",
    "cookie": "^0.5.0",
